{
  "src": "./sources",
  "dest": "./build",

  "copy": [
    {
      "src": "libs/**/*",
      "dest": "libs"
    }
  ],

  "images": {
    "src": "images/**/*",
    "dest": "images"
  },

  "javascripts": {
    "src": "js/**/*.js",
    "dest": "js"
  },

  "sass": {
    "src": "sass/**/*.s+(a|c)ss",
    "dest": "css"
  }
}
